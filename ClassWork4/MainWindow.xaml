<Window x:Class="ClassWork4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClassWork4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="DeepSkyBlue" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <TabControl>
        <TabItem Header="Home"
                 Padding="15"
                 FontSize="22"
                 FontFamily="Georgia">
            <Grid Background="SeaGreen">
                <TextBlock>Home</TextBlock>
            </Grid>
        </TabItem>
        <TabItem Header="About"
                 Padding="15"
                 FontSize="22"
                 FontFamily="Georgia">
            <Grid Background="Cyan">
                <TextBlock>About</TextBlock>
            </Grid>
        </TabItem>
        <TabItem Header="News"
                 Padding="15"
                 FontSize="22"
                 FontFamily="Georgia">
            <Grid Background="Green">
                <TextBlock>News</TextBlock>
            </Grid>
        </TabItem>
        <TabItem Header="Contact"
                 Padding="15"
                 FontSize="22"
                 FontFamily="Georgia">
            <Grid Background="CadetBlue">
                <TextBlock>Contact</TextBlock>
            </Grid>
        </TabItem>
    </TabControl>
</Window>
